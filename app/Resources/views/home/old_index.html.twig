        }

        .cw-tags a:hover {
            background: #97BC23;
            border-color: #97BC23;
            color: #fff;
            text-decoration: none;
        }

        aside .cw-tags a {
            color: gray;
            background-color: transparent;
        }

        address a {
            color: #999;
        }

        .list-recent a {
            color: #bfbfbf;
            display: inline-block;
            padding: 3px 0;
            font-size: 14px;
            line-height: 22px;
            margin: 0 1px 6px 0;

        }

        .list-recent a:hover {
            color: #97BC23;
            text-decoration: none;
        }

        .subfooter {
            background-color: #000;
            font-size: 14px;
            color: #888;
        }

        .subfooter a {
            color: #97BC23;
            -webkit-transition: all .2s ease-in-out;
            -moz-transition: all .2s ease-in-out;
            -ms-transition: all .2s ease-in-out;
            -o-transition: all .2s ease-in-out;
            transition: all .2s ease-in-out;
            text-decoration: none;
        }

        .rights{
